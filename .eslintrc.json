{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "@react-native-community"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "quotes": "off",
        "comma-dangle": "off",
        "no-shadow": "off"
    }
}
